# auto-news environment configuration
# Copy to workspace/airflow/config/.env

# LLM Provider: openai, google, ollama
LLM_PROVIDER=openai
OPENAI_MODEL=gpt-4o-mini
OPENAI_API_KEY=

# Content sources (comma-separated): Twitter,Reddit,Article,Youtube,RSS
CONTENT_SOURCES=Article,RSS

# Notion (optional)
NOTION_TOKEN=
NOTION_ENTRY_PAGE_ID=

# Embedding
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=text-embedding-ada-002
